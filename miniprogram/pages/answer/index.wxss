/* pages/answer/index.wxss */
.questionnaire-name {
  font-size: 15px;
}
.record-box {
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  width: 100%;
  padding: 20rpx;
}
.record{
  width: 70rpx;
  height: 70rpx;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  border-width: 2rpx;
  border-style: solid;
  border-radius: 42rpx;
  background-color:#cfd4d8;
  color: #ffffff;
  margin: 25rpx;
  font-size: 28rpx;
  box-sizing: border-box;
}